  "attributes": [
    {
      "parameter": "type",
      "directions": "行内表单模式",
      "type": "String",
      "optional": "search / login / form",
      "default": "search",
      "keyPoint": true
    },
    {
      "parameter": "model",
      "directions": "表单数据对象",
      "type": "Object",
      "optional": "—",
      "default": "—"
    },
    {
      "parameter": "rules",
      "directions": "表单验证规则",
      "type": "Object",
      "optional": "—",
      "default": "—"
    },
    {
      "parameter": "column",
      "directions": "一行 Form Item 的数量",
      "type": "Number",
      "optional": "—",
      "default": "—",
      "keyPoint": true
    },
    {
      "parameter": "label-position",
      "directions": "表单域标签的位置，如果值为 left 或者 right 时，则需要设置 label-width。",
      "type": "String",
      "optional": "right / left / top",
      "default": "right"
    },
    {
      "parameter": "label-width",
      "directions": "用于控制 \"el-form-item label\" 的宽度。支持 auto。",
      "type": "String",
      "optional": "—",
      "default": "—"
    },
    {
      "parameter": "label-suffix",
      "directions": "用于控制 \"el-form-item label\" 的后缀",
      "type": "String",
      "optional": "—",
      "default": "—"
    },
    {
      "parameter": "hide-required-asterisk",
      "directions": "是否隐藏必填字段的标签旁边的红色星号",
      "type": "Boolean",
      "optional": "—",
      "default": "false"
    },
    {
      "parameter": "show-message",
      "directions": "是否显示校验错误信息",
      "type": "Boolean",
      "optional": "—",
      "default": "true"
    },
    {
      "parameter": "status-icon",
      "directions": "是否在输入框中显示校验结果反馈图标",
      "type": "Boolean",
      "optional": "—",
      "default": "false"
    },
    {
      "parameter": "validate-on-rule-change",
      "directions": "是否在 rules 属性改变后立即触发一次验证",
      "type": "Boolean",
      "optional": "—",
      "default": "true"
    },
    {
      "parameter": "level",
      "directions": "阴影级别",
      "type": "Object",
      "optional": "1 / 2 / 3",
      "default": "—",
      "keyPoint": true
    },
    {
      "parameter": "border",
      "directions": "边框线",
      "type": "Object",
      "optional": "—",
      "default": "false",
      "keyPoint": true
    },
    {
      "parameter": "radius",
      "directions": "圆角",
      "type": "Object",
      "optional": "—",
      "default": "false",
      "keyPoint": true
    },
    {
      "parameter": "size",
      "directions": "用于控制该表单内组件的尺寸。如果type是 \"login\" 或 \"form\" 默认值 medium，type是 \"search\" 默认值 small。",
      "type": "String",
      "optional": "medium / small / mini",
      "default": "—"
    },
    {
      "parameter": "disabled",
      "directions": "是否禁用该表单内的所有组件。若设置为 true，则表单内组件上的 disabled 属性不再生效。",
      "type": "Boolean",
      "optional": "-",
      "default": "false"
    }
  ],
  "methods": [
    {
      "name": "validate",
      "directions": "对整个表单进行校验的方法，参数为一个回调函数。该回调函数会在校验结束后被调用，并传入两个参数：是否校验成功和未通过校验的字段。若不传入回调函数，则会返回一个 promise",
      "parameter": "Function(callback: Function(boolean, object))"
    },
    {
      "name": "validateField",
      "directions": "对部分表单字段进行校验的方法",
      "parameter": "Function(props: array | string, callback: Function(errorMessage: string))"
    },
    {
      "name": "resetFields",
      "directions": "对整个表单进行重置，将所有字段值重置为初始值并移除校验结果",
      "parameter": "—"
    },
    {
      "name": "clearValidate",
      "directions": "移除表单项的校验结果。传入待移除的表单项的 prop 属性或者 prop 组成的数组，如不传则移除整个表单的校验结果",
      "parameter": "Function(props: array | string)"
    }
  ],
  "events": [
    {
      "name": "validate",
      "directions": "任一表单项被校验后触发",
      "parameter": "被校验的表单项 prop 值，校验是否通过，错误消息（如果存在）"
    }
  ],
